            <DropShadow />
         </effect>
         <children>
            <ImageView fx:id="imagePane" fitHeight="178.0" fitWidth="162.0" layoutX="139.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" />
            <ScrollPane fx:id="scrollbar" prefHeight="369.0" prefWidth="142.0">
